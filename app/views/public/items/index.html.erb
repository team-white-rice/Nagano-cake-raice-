<div class="container">
  <div class="row">
    <div class="col-2">
      <div>ジャンル検索</div>
    </div>
    <div class="col-10">
      <h4>商品一覧</h4>
      <% @menus.each do |menu| %>
        <%= image_tag menu.menu_image, size: "200x200" %>
        <%= menu.name %>
        <%= '¥',menu.price %>
      <% end %>
    </div>
  </div>
</div>